import java.util.Date;

public class Transacao {
    private String tipo;
    private double valor;
    private Conta conta;
    private Date data;

    public Transacao(String tipo, double valor, Conta conta) {
        this.tipo = tipo;
        this.valor = valor;
        this.conta = conta;
        this.data = new Date();
    }

    public String getTipo() {
        return tipo;
    }

    public double getValor() {
        return valor;
    }

    public Conta getConta() {
        return conta;
    }

    public Date getData() {
        return data;
    }
}